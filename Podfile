  use_frameworks!
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'SnapKit',    '~> 4.2'
  pod 'FlagKit',    '~> 2.0.1'
  pod 'Swinject',   '~> 2.4.0'
end

target 'WhichCountry Widget' do
    use_frameworks!
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'
    pod 'FlagKit',    '~> 2.0.1'
end
